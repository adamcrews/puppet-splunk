{
  "name": "huit-splunk",
  "version": "1.7.1",
  "author": "Tim Hartmann",
  "summary": "Manage Splunk Agents and Servers",
  "license": "MIT",
  "source": "git://github.com/huit/puppet-splunk.git",
  "project_page": "https://forge.puppetlabs.com/huit/splunk",
  "issues_url": "https://github.com/huit/puppet-splunk/issues",
  "description": "This Module manages Splunk Indexer, Search Heads, SLF and Heavy Forwarders as well as a number of apps",
  "dependencies": [
    {"name":"puppetlabs/inifile","version_requirement":">=1.0.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">=3.2.0"}
    {"name":"puppetlabs/acl","version_requirement":">=1.1.2"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem":"RedHat",
      "operatingsystemrelease":[ "5.0", "6.0" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "12.04", "10.04" ]
    },
    "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2003",
        "Server 2003 R2",
        "Server 2008",
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "7",
        "8"
      ]
    }
  ]
}
